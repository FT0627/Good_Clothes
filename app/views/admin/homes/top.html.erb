<div class="container">
  <dvi class="contents-title text-center">
    <h3 class="my-3">ユーザー</h3>
    <div class="row">
      <% @users.each do |user| %>
        <div class="col-md-6 col-lg-3">
          <div class="card">
            <%= image_tag user.get_profile_image(300,300) %>
            <div class="card-body">
            <%= link_to admin_user_path(user) do %>
              <p class="card-title"><%= user.name %></p>
            <% end %>
            </div>
          </div>
        </div>
      <% end %>
    </div>
  </dvi>
</div>
