<div class="container main-content">
  <dvi class="contents-title text-center">
    <h3 class="my-3">ユーザー</h3>
    <div class="row">
      <% @users.each do |user| %>
        <div class="col-md-6 col-lg-3">
          <div class="card">
            <%= image_tag user.get_profile_image(300,300) %>
            <div class="card-body">
              <p class="card-title"><%= link_to user.name, admin_user_path(user) %></p>
            </div>
          </div>
        </div>
      <% end %>
    </div>
  </dvi>
</div>
